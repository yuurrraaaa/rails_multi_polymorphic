<p id="notice"><%= notice %></p>

<h1>Researches</h1>

<table>
  <thead>
    <tr>
      <th>Procedure</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Doctor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @researches.each do |research| %>
      <tr>
        <td><%= research.procedure_id %></td>
        <td><%= research.start_time %></td>
        <td><%= research.end_time %></td>
        <td><%= research.doctor %></td>
        <td><%= link_to 'Show', research %></td>
        <td><%= link_to 'Edit', edit_research_path(research) %></td>
        <td><%= link_to 'Destroy', research, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Research', new_research_path %>
